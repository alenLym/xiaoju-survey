XIAOJU_SURVEY_MONGO_DB_NAME=xiaojuSurvey
# 小桔问卷数据库地址
XIAOJU_SURVEY_MONGO_URL=mongodb://localhost:27017
# 小桔问卷数据库认证源
XIAOJU_SURVEY_MONGO_AUTH_SOURCE=admin

# 小桔问卷响应数据加密密钥
